{
  "info": {
    "version": "",
    "description": ""
  },
  "settings": {
    "exchangeRate": {
      "baseCurrency": "EUR"
    }
  },
  "logger": {
    "directory": "D:\\Coding\\01. Visual Studio Projects\\LittleMarketHelper\\LittleMarketHelper\\Logs",
    "maxLogFiles": 5
  },
  "parser": {
    "type": {
      "degiro": {
        "name": "DEGIRO",
        "fileName": "Portfolio",
        "fileExtension": ".csv"
      }
    }
  },
  "httpRequest": {
    "quoteByFullTicker": {
      "yahooFinance": [
        {
          "method": "get",
          "url": "https://query1.finance.yahoo.com/v8/finance/chart/%%%PLACEHOLDER%%%",
          "key": {
            "price": "regularMarketPrice",
            "ccy": "currency",
            "timestamp": "regularMarketTime"
          }
        }
      ]
    },
    "exchangeRates": {
      "method": "get",
      "url": "https://cdn.jsdelivr.net/gh/fawazahmed0/currency-api@1/latest/currencies/eur.json",
      "key": "eur"
    },
    "tickerByIsin": {
      "openFigi": {
        "method": "post",
        "url": "https://api.openfigi.com/v3/mapping",
        "data": "[{\"idType\":\"ID_ISIN\",\"idValue\":\"%%%PLACEHOLDER%%%\"}]",
        "key": {
          "ticker": "ticker",
          "exchange": "exchCode"
        }
      }
    },
    "networkConnection": {
      "method": "get",
      "url": "https://catfact.ninja/fact",
      "key": "fact"
    }
  }
}
